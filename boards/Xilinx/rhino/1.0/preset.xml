<ip_presets schema="1.0">

	<ip_preset preset_proc_name="ps_pmc_fixed_io_preset">
		<ip vendor="xilinx.com" library="ip" name="versal_cips" version="*" ip_interface="FIXED_IO">
			<user_parameters>
				<user_hier_parameter name="CONFIG.PS_PMC_CONFIG">

					<user_hier_parameter name="PMC_I2CPMC_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="50 .. 51"/>
						</user_hier_parameter>
					</user_hier_parameter>
					
					<user_hier_parameter name="PMC_MIO12">
						<user_parameter name="AUX_IO" value="0"/>
						<!-- <user_parameter name="OUTPUT_DATA" value="high"/> -->
						<user_parameter name="PULL" value="pullup"/>
						<user_parameter name="USAGE" value="GPIO"/>
						<!-- <user_parameter name="DRIVE_STRENGTH" value="4mA"/> -->
						<user_parameter name="DIRECTION" value="out"/>
					</user_hier_parameter>
					
					<user_hier_parameter name="PMC_OSPI_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="0 .. 11"/>
						</user_hier_parameter>
						<user_parameter name="MODE" value="Single"/>
					</user_hier_parameter>
					
					<user_hier_parameter name="PS_CAN0_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="46 .. 47"/>
						</user_hier_parameter>
					</user_hier_parameter>
					<!-- <user_parameter name="PS_CRL_CAN0_REF_CTRL_FREQMHZ" value="160"/> -->
					
					<user_hier_parameter name="PS_CAN1_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PS_MIO" value="16 .. 17"/>
						</user_hier_parameter>
					</user_hier_parameter>
					<!-- <user_parameter name="PS_CRL_CAN1_REF_CTRL_FREQMHZ" value="160"/> -->

					<user_hier_parameter name="PS_ENET0_MDIO">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PS_MIO" value="24 .. 25"/>
						</user_hier_parameter>
					</user_hier_parameter>
				
					<user_hier_parameter name="PS_ENET0_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PS_MIO" value="0 .. 11"/>
						</user_hier_parameter>
					</user_hier_parameter>

					<user_hier_parameter name="PS_I2C1_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="32 .. 33"/>
						</user_hier_parameter>
					</user_hier_parameter>
					
					<user_hier_parameter name="PS_I2CSYSMON_PERIPHERAL">
						<user_parameter name="ENABLE" value="0"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="35 .. 37"/>
						</user_hier_parameter>
					</user_hier_parameter>

<!-- 					<user_hier_parameter name="PS_PCIE_RESET">
						<user_parameter name="ENABLE" value="1"/>
					</user_hier_parameter> -->

<!-- 					<user_hier_parameter name="PS_PCIE_EP_RESET1_IO">
						<user_parameter name="PMC_MIO" value="38"/>
					</user_hier_parameter>

					<user_hier_parameter name="PS_PCIE_EP_RESET2_IO">
						<user_parameter name="PS_MIO" value="19"/>
					</user_hier_parameter> -->
					
					<user_hier_parameter name="PS_UART0_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="42 .. 43"/>
						</user_hier_parameter>
					</user_hier_parameter>
					
					<user_hier_parameter name="PS_UART1_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PS_MIO" value="12 .. 13"/>
						</user_hier_parameter>
					</user_hier_parameter>

					<user_hier_parameter name="PS_USB3_PERIPHERAL">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="13 .. 25"/>
						</user_hier_parameter>
					</user_hier_parameter>

					<user_hier_parameter name="PS_SPI1">
						<user_parameter name="GRP_SS0_ENABLE" value="1"/>
						<user_hier_parameter name="GRP_SS0_IO">
							<user_parameter name="PS_MIO" value="21"/>
						</user_hier_parameter>
<!-- 						<user_hier_parameter name="GRP_SS1_IO">
							<user_parameter name="PMC_MIO" value="14"/>
						</user_hier_parameter>
						<user_hier_parameter name="GRP_SS2_IO">
							<user_parameter name="PMC_MIO" value="13"/>
						</user_hier_parameter> -->
						<user_parameter name="PERIPHERAL_ENABLE" value="1"/>
						<user_hier_parameter name="PERIPHERAL_IO">
							<user_parameter name="PS_MIO" value="18 .. 23"/>
						</user_hier_parameter>
					</user_hier_parameter>
					
					<!-- <user_parameter name="DEVICE_INTEGRITY_MODE" value="{Sysmon temperature voltage and external IO monitoring}"/> -->
					<!-- <user_parameter name="SMON_PMBUS_ADDRESS" value="0x18"/> -->
					<user_parameter name="SMON_INTERFACE_TO_USE" value="PMBus"/>
					
					<user_parameter name="PMC_REF_CLK_FREQMHZ" value="33.3333"/>
					
					<user_parameter name="PS_GEN_IPI0_ENABLE" value="1"/>
					<user_parameter name="PS_GEN_IPI0_MASTER" value="A72"/>
					<user_parameter name="PS_GEN_IPI1_ENABLE" value="1"/>
					<user_parameter name="PS_GEN_IPI1_MASTER" value="R5_0"/>
					<user_parameter name="PS_GEN_IPI2_ENABLE" value="1"/>
					<user_parameter name="PS_GEN_IPI2_MASTER" value="R5_1"/>
					<user_parameter name="PS_GEN_IPI3_ENABLE" value="1"/>
					<user_parameter name="PS_GEN_IPI3_MASTER" value="A72"/>
					<user_parameter name="PS_GEN_IPI4_ENABLE" value="1"/>
					<user_parameter name="PS_GEN_IPI4_MASTER" value="A72"/>
					<user_parameter name="PS_GEN_IPI5_ENABLE" value="1"/>
					<user_parameter name="PS_GEN_IPI5_MASTER" value="A72"/>
					<user_parameter name="PS_GEN_IPI6_ENABLE" value="1"/>
					<user_parameter name="PS_GEN_IPI6_MASTER" value="A72"/>
					
					<!-- <user_parameter name="PS_TTC0_PERIPHERAL_ENABLE" value="1"/>
					<user_parameter name="PS_TTC1_PERIPHERAL_ENABLE" value="1"/> -->
					<user_parameter name="PS_TTC2_PERIPHERAL_ENABLE" value="1"/>
					<!-- <user_parameter name="PS_TTC3_PERIPHERAL_ENABLE" value="1"/> -->

					<user_hier_parameter name="PS_TTC2_WAVEOUT">
						<user_parameter name="ENABLE" value="1"/>
						<user_hier_parameter name="IO">
							<user_parameter name="PMC_MIO" value="45"/>
						</user_hier_parameter>
					</user_hier_parameter>
		
				</user_hier_parameter>
			</user_parameters>
		</ip>
	</ip_preset>

	<ip_preset preset_proc_name="LPDDR4_Controller0_preset">
		<ip vendor="xilinx.com" library="ip" name="axi_noc" version="*">
			<user_parameters>
				<user_parameter name="CONFIG.CONTROLLERTYPE" value="LPDDR4_SDRAM"/>
				<user_parameter name="CONFIG.MC_FREQ_SEL" value="MEMORY_CLK_FROM_SYS_CLK"/>
				<user_parameter name="CONFIG.MC_IP_TIMEPERIOD0_FOR_OP" value="5000"/>				
				<user_parameter name="CONFIG.MC_OP_TIMEPERIOD0" value="541"/> 
				<user_parameter name="CONFIG.MC_SYSTEM_CLOCK" value="Differential"/>  
				<user_parameter name="CONFIG.MC_MEMORY_SPEEDGRADE" value="LPDDR4-4267"/>
				<user_parameter name="CONFIG.MC_DATAWIDTH" value="32"/>
				<user_parameter name="CONFIG.MC_NO_CHANNELS" value="Dual"/>
				<user_parameter name="CONFIG.MC_ROWADDRESSWIDTH" value="17"/>
				<user_parameter name="CONFIG.MC_LP4_PIN_EFFICIENT" value="true"/>
			</user_parameters>
		</ip>
	</ip_preset>

	<ip_preset preset_proc_name="LPDDR4_Controller1_preset">
		<ip vendor="xilinx.com" library="ip" name="axi_noc" version="*">
			<user_parameters>
				<user_parameter name="CONFIG.CONTROLLERTYPE" value="LPDDR4_SDRAM"/>
				<user_parameter name="CONFIG.MC_FREQ_SEL" value="MEMORY_CLK_FROM_SYS_CLK"/>
				<user_parameter name="CONFIG.MC_IP_TIMEPERIOD0_FOR_OP" value="5000"/>				
				<user_parameter name="CONFIG.MC_OP_TIMEPERIOD0" value="541"/>
				<user_parameter name="CONFIG.MC_SYSTEM_CLOCK" value="Differential"/> 
				<user_parameter name="CONFIG.MC_LP4_PIN_EFFICIENT" value="true"/>
				<user_parameter name="CONFIG.MC_NO_CHANNELS" value="Dual"/>
				<user_parameter name="CONFIG.MC_MEMORY_SPEEDGRADE" value="LPDDR4-4267"/>
				<user_parameter name="CONFIG.MC_DATAWIDTH" value="32"/>
				<user_parameter name="CONFIG.MC_ROWADDRESSWIDTH" value="17"/>
				

			</user_parameters>
		</ip>
	</ip_preset>

	<ip_preset preset_proc_name="LPDDR4_Controller2_preset">
		<ip vendor="xilinx.com" library="ip" name="axi_noc" version="*">
			<user_parameters>
				<user_parameter name="CONFIG.CONTROLLERTYPE" value="LPDDR4_SDRAM"/>  
